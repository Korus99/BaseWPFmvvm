<Window x:Class="BaseWpfMvvm_App.Common.Control.HelpDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BaseWpfMvvm_App.Common.Control"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="HelpDialog" Height="400" Width="400">
    <Grid>
        <Label FontSize="14" Content="{Binding HelpMessage}" VerticalAlignment="Center" HorizontalAlignment="Center"></Label>
    </Grid>
</Window>
